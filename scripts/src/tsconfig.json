{
  "compilerOptions": {
    "lib": ["esnext"],
    "target": "ES5",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
